[user]
	name = isudzumi
	email = isudzumi@gmail.com

[alias]
	st = status
	cm = commit
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	cancel = reset --soft HEAD^
	b = branch
	co = checkout
    cob = checkout -b
    com = checkout master
    plr = pull --rebase
    fp = !git fetch -p && git bm | grep -v \\* | xargs -I % git branch -d %
