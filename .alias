alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
if type nvim > /dev/null 2>&1; then
  alias vi=nvim
  alias less=/usr/local/Cellar/neovim/0.2.2_1/share/nvim/runtime/macros/less.sh
fi
alias cls='clear'
if type hub > /dev/null 2>&1; then
  alias git=hub
fi

alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias so='source'

if type rg > /dev/null 2>&1; then
  alias grep='rg'
fi

if type ghq > /dev/null 2>&1; then
  alias gl='ghq look'
  alias gg='ghq get -p'
fi
